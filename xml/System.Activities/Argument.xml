    <Member MemberName="EvaluationOrder">
      <MemberSignature Language="C#" Value="public int EvaluationOrder { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance int32 EvaluationOrder" />
      <MemberType>Property</MemberType>
      <AssemblyInfo>
